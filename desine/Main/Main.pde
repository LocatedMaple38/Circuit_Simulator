  multimeterModeInt[0] = '1';

  surface.setResizable(true);
  surface.setLocation(0, 0);

  if (simBool == false) {
    surface.setTitle("Design "+Name);
  } else {
    surface.setTitle("Simulate "+Name);
  }

  addSetup();
  fileSetup();
  simSetup();
}

