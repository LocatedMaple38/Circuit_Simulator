    fileSave();
  }

  if (mouseX>xItemAdd && mouseX<xItemAdd+widthItemAdd && mouseY>yItemAdd && mouseY<yItemAdd+heightItemAdd) {
    itemAddBool = itemAddBool ? false : true;
    fileBool = false;
